{
  "name": "test-u",
  "version": "0.1.0",
  "description": "Utils (test git-submodules)",
  "private": true,
  "repository": "VovanR/test-u",
  "scripts": {
    "test": "xo",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "main": "index.js",
  "author": "Vladimir Rodkin <mail@vovanr.com> (https://vovanr.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VovanR/test-u/issues"
  },
  "homepage": "https://github.com/VovanR/test-u#readme",
  "dependencies": {},
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "mversion": "^2.0.0",
    "xo": "^0.28.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "rules": {
      "comma-dangle": "off",
      "unicorn/filename-case": "off"
    },
    "semicolon": false,
    "space": true
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
